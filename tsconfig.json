{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"lib": ["es2015", "es2017", "dom"],
		"sourceMap": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": false,
		"noImplicitThis": false,
		"noImplicitAny": false,
		"strictNullChecks": true,
		"downlevelIteration": true
	},
	"include": [
		"*.ts"
	]
}